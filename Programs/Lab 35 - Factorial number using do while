package com.dowhile;

import java.util.Scanner;

public class DoFact {

	public static void main(String[] args) {
		int num,a;
		double fact=1;
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter any Number = ");
		num = sc.nextInt();
		a=1;
		do {
			fact=fact*a;
			a++;
		}while(a<=num);
		System.out.println("Factorial = "+fact);
	}

}
