package com.loops;

import java.util.Scanner;

public class PrimeuptoN {

	public static void main(String[] args) {
		int num,fc;
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter any number = ");
		num = sc.nextInt();
		for(int i = 1; i <= num; i++) {
			fc=0;
			for(int j = 1; j <= i; j++) {
				if(i%j==0)
					fc++;
			}
			if(fc==2)
				System.out.print(i+" ");
		}	
	}
}
