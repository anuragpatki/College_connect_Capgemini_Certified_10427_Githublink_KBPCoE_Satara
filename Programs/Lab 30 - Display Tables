package com.loops;

import java.util.Scanner;

public class Tables {

	public static void main(String[] args) {
		int num,table;
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter any number = ");
		num = sc.nextInt();
		for(int i =1; i<=10; i++) {
			table=num*i;
			System.out.println(num+" X "+i+" = "+table);
		}
	}

}
	
