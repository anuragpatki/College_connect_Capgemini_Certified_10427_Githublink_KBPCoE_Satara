package com.loops;

public class ReversePrime {

	public static void main(String[] args) {
		int num,fc,k=0;
		System.out.println("Prime Numbers");
		
		for(num = 100; num >= 1; num--) {
			fc=0;
			for(int i = 1; i <= num; i++) {
				if(num%i==0)
					fc++;
			}
			if(fc==2) {
				System.out.println(num);
				k++;
				if(k==5)
					break;
			}
		}
		
	}

}
