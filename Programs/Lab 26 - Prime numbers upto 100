package com.loops;

public class Primeupto100 {

	public static void main(String[] args) {
		int num,fc;
		System.out.println("Prime numbers");
		for(num = 1; num <= 100; num++) {
			fc = 0;
			for (int i = 1; i <= num; i++) {
				if(num%i==0)
					fc++;
			}
			if(fc==2) {
				if(num<97)
					System.out.print(num+",");
				else
					System.out.println(num);
			}
		}
	}

}
